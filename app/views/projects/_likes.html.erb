
<%= project.likes.count %>

<% if user_signed_in? && current_user.likes?(project) %>
<%= link_to project_like_path(project), method: :delete, remote: true do %>
<i class="fa fa-lightbulb-o"></i>
<% end %>
<% else %>
<%= link_to project_like_path(project), method: :post, remote: true do %>
<i class="fa fa-lightbulb-o"></i>
<% end %>
<% end %>
